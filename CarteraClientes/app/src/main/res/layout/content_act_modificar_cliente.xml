<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".ActModificarCliente"
    tools:showIn="@layout/act_modificar_cliente">
    <TextView
        android:id="@+id/textView7M"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="ID"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/edtNID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:hint="Ingrese el ID"
        app:layout_constraintTop_toBottomOf="@+id/textView7M"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView2M"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Nombre"
        app:layout_constraintTop_toBottomOf="@+id/edtNID"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/edtNNombre"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:hint="Ingrese su nuevo nombre ..."
        app:layout_constraintTop_toBottomOf="@+id/textView2M"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView3M"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Direccion"
        app:layout_constraintTop_toBottomOf="@+id/edtNNombre"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/edtNDireccion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:hint="Ingrese su nueva Direccion ..."
        app:layout_constraintTop_toBottomOf="@+id/textView3M"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView4M"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Email"
        app:layout_constraintTop_toBottomOf="@+id/edtNDireccion"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/edtNEmail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:hint="Ingrese su nuevo Email ..."
        app:layout_constraintTop_toBottomOf="@+id/textView4M"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView5M"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Telefono"
        app:layout_constraintTop_toBottomOf="@+id/edtNEmail"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/edtNTelefono"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:hint="Ingrese su nuevo Telefono ..."
        app:layout_constraintTop_toBottomOf="@+id/textView5M"
        tools:layout_editor_absoluteX="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>





